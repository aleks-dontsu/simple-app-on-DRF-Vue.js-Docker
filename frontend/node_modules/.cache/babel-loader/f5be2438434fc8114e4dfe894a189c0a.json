ApD,IAAA,WAAW,EAAC,MAAwC;;aAAjB,KAAA,CAAA,UAAA,CAAW,IAAX,GAAe,M;;AAAE,GAAvE,E,IAAA,E;;AAAA,G,iBAAsD,KAAA,CAAA,UAAA,CAAW,I;YAA3B;MAGlC,E,gBAFJ,YAAA,CAA2E,OAA3E,EAA2E;AAApE,IAAA,IAAI,EAAC,MAA+D;AAAxD,IAAA,WAAW,EAAC,QAA4C;;aAAnB,KAAA,CAAA,UAAA,CAAW,MAAX,GAAiB,M;;AAAE,GAA3E,E,IAAA,E;;AAAA,G,iBAAwD,KAAA,CAAA,UAAA,CAAW,M;YAA3B;MAEpC,EADJ,YAAA,CAA4C,QAA5C,EAA4C;AAAnC,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,QAAA,CAAA,SAAA,EAAF;AAAA,KAAA;AAA8B,GAA5C,EAA6B,QAA7B,CACI,CAjCN,C","sourcesContent":["<template>\n  <div>\n    <button>&lt;</button>\n    <button>&gt;</button>\n    <table>\n      <thead>\n      <tr>\n        <th>ID</th>\n        <th>Vendor</th>\n        <th>Model</th>\n        <th>Year</th>\n        <th>Volume</th>\n        <th>Remove</th>\n      </tr>\n      </thead>\n      <transition-group name=\"flip-list\" tag=\"tbody\">\n        <tr v-for=\"car in cars\" v-bind:key=\"car.id\">\n          <td>{{ car.id }}</td>\n          <td>{{ car.vendor }}</td>\n          <td>{{ car.model }}</td>\n          <td>{{ car.year }}</td>\n          <td>{{ car.volume }}</td>\n          <td><button @click=\"removeCar(car)\">-</button></td>\n        </tr>\n      </transition-group>\n    </table>\n\n    <button @click=\"shuffleCars()\">Shuffle</button>\n\n    <input type=\"text\" placeholder=\"vendor\" v-model=\"currentCar.vendor\">\n    <input type=\"text\" placeholder=\"model\" v-model=\"currentCar.model\">\n    <input type=\"text\" placeholder=\"year\" v-model.number=\"currentCar.year\">\n    <input type=\"text\" placeholder=\"volume\" v-model.number=\"currentCar.volume\">\n    <button @click=\"createCar()\">Create</button>\n  </div>\n</template>\n\n<script>\nimport { shuffle } from 'lodash';\n\nexport default {\n  name: 'Home',\n\n  data() {\n    return {\n      cars: [],\n      currentCar: {},\n    };\n  },\n  async created() {\n    await this.fetchCars();\n  },\n\n  methods: {\n    async fetchCars() {\n      const response = await fetch('http://127.0.0.1:8000/api/cars/');\n      const { results } = await response.json();\n      this.cars = results;\n    },\n    async createCar() {\n      const response = await fetch('http://127.0.0.1:8000/api/cars/', {\n        method: 'POST',\n        headers: {\n          // 'Accept': 'application/json',\n          'Content-Type': 'application/json',\n        },\n        body: JSON.stringify(this.currentCar),\n      });\n\n      if (response.status !== 201) {\n        alert(JSON.stringify(await response.json(), null, 2));\n      }\n\n      await this.fetchCars();\n    },\n    async removeCar(car) {\n      const { id } = car;\n      const response = await fetch(`http://127.0.0.1:8000/api/cars/${id}/`, {\n        method: 'DELETE',\n        headers: {\n          // 'Accept': 'application/json',\n          'Content-Type': 'application/json',\n        },\n      });\n\n      if (response.status !== 204) {\n        alert(JSON.stringify(await response.json(), null, 2));\n      }\n\n      await this.fetchCars();\n    },\n\n    shuffleCars() {\n      this.cars = shuffle(this.cars);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.flip-list-move {\n  transition: transform 0.3s;\n}\n</style>\n"],"sourceRoot":""}]}